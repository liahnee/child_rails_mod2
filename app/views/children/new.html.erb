<h1> Adopt to Your Child! </h1>
<%= form_for @child do |f| %>
    <label> First Name: </label>
    <%= f.text_field :first_name %><br>
    <label> Last Name: </label>
    <%= f.text_field :last_name %><br>
    <%= f.submit, "Adopt A Child" %>
<% end %>

<%= link_to "Back to children contacts", children_path %>